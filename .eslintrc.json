{    
    "extends": "react-app",
    "parserOptions": {
        "ecmaVersion": 2020,
      "sourceType": "module"
      
    },
    "env": {
      "es6": true,
      "jest": true,
      "node": true
    },
    "rules": {
      "eol-last": "error",
      "eqeqeq": [
        "error",
        "always"
      ],
      
      "new-cap": "warn",
      "no-console": "off",
      "no-multi-spaces": [
        "warn",
        {
          "exceptions": {
            "VariableDeclarator": true
          }
        }
      ],
      "no-redeclare": [
        "error",
        {
          "builtinGlobals": true
        }
      ],
      "no-template-curly-in-string": "error",
      "no-trailing-spaces": "warn",
      "no-undefined": "off",
      "quotes": [
        "warn",
        "single",
        {
          "allowTemplateLiterals": true
        }
      ],
      "semi": "error"
    }
  }